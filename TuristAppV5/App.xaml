<Application
    x:Class="TuristAppV5.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TuristAppV5">
	<Application.Resources>
		<MenuFlyout x:Key="FlyoutBase1"/>
        <SolidColorBrush x:Key="AppBarItemBackgroundThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="AppBarItemDisabledForegroundThemeBrush" Color="#FFED6B06" />
        <SolidColorBrush x:Key="AppBarItemForegroundThemeBrush" Color="#FFED6B06" />
        <SolidColorBrush x:Key="AppBarItemPointerOverBackgroundThemeBrush" Color="#FFED6B06" />
        <SolidColorBrush x:Key="AppBarItemPointerOverForegroundThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="AppBarItemPressedForegroundThemeBrush" Color="Transparent" />
        
    </Application.Resources>

</Application>
